$(".facts").click(function(){
    $("#div-home").hide();
    $("#div-pic").hide();
    $("#div-types").hide();
    $("#div-facts").show();
})

$(".types").click(function(){
    $("#div-pic").hide();
    $("#div-home").hide();
    $('#div-facts').hide();
    $("#div-types").show();
})

$(".pictures").click(function(){
    $("#div-pic").show();
    $("#div-home").hide();
    $('#div-facts').hide();
    $("#div-types").hide();
})

$(".home").click(function(){
    $("#div-pic").hide();
    $("#div-home").show();
    $('#div-facts').hide();
    $("#div-types").hide();
})







</script>
</html>
