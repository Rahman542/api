<script>

    function isNaturalNumber(x,y){
      if (isNaN(x) && isNaN(y)){
        return true;
      }else if(x == "" || y == "" ){
        return false;
      }else{
            return false;
    }
}
$('#btn_1').click(function(){
    var serialized_data = $("#dogtranslator").serialize();
	  console.log(serialized_data);
   $.post("https://api.funtranslations.com/translate/doge.json",serialized_data,function(data){
   console.log(data);
   var translate = data["contents"]["translated"]
   $("#doge").html(translate)
   });
});

$('#btn_3').click(function(){
  console.log('testing')
  $.getJSON("https://random.dog/woof.json", function(data){
    console.log('API call')
    console.log(data['url'])
    var dog= data['url']
    $('#dogPicture').html('<img src= ' + dog + '>')

  });
  $('#btn_10').click(function(){
  
  let message = "Thank you for your response."
  $('#textDisplayed10').html(message);

  
});

});

$(document).ready(function(){
    $("#div-facts").hide();
    $("#div-types").hide();
    $("#div-pic").hide();
    $("#div-home").show();
    
});

